{
  "name": "@justapithecus/quarry-sdk",
  "version": "0.5.0",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "biome check .",
    "format": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:types": "tsd"
  },
  "peerDependencies": {
    "puppeteer": "^24",
    "puppeteer-extra": "^3"
  },
  "devDependencies": {
    "fast-check": "^4.5.3",
    "puppeteer": "^24.37.2",
    "puppeteer-extra": "^3.3.6",
    "tsd": "^0.32.0",
    "tsdown": "^0.20.3"
  },
  "tsd": {
    "directory": "test/emit/00-types",
    "typingsFile": "src/index.ts"
  }
}
