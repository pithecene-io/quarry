{
  "name": "@justapithecus/quarry-executor-node",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "quarry-executor": "./dist/bin/executor.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && chmod +x dist/bin/executor.js",
    "bundle": "mkdir -p dist/bundle ../quarry/executor/bundle && tsx scripts/bundle.ts && chmod +x ../quarry/executor/bundle/executor.mjs",
    "lint": "biome check .",
    "format": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@justapithecus/quarry-sdk": "workspace:*",
    "@msgpack/msgpack": "^3.1.3"
  },
  "peerDependencies": {
    "puppeteer": "^24",
    "puppeteer-extra": "^3",
    "puppeteer-extra-plugin-adblocker": "^2",
    "puppeteer-extra-plugin-stealth": "^2"
  },
  "peerDependenciesMeta": {
    "puppeteer-extra-plugin-adblocker": {
      "optional": true
    }
  },
  "devDependencies": {
    "esbuild": "^0.27.3",
    "puppeteer": "^24.37.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  }
}
